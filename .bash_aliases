# classic
alias ee=exit
alias c='printf "\\e[H\\e[2J"'
alias cf=clear
alias f=fg
alias l='ls -lh'
alias la='ls -lha'
alias ret='echo $?'

# movements
alias cd.='cd ..'
alias cd..='cd ../..'
alias cd...='cd ../../..'
alias cd....='cd ../../../..'

# config
alias brc='vim ~/.bashrc'
alias brca='vim ~/.bash_aliases'
alias sbrc='source ~/.bashrc'
alias vrc='vim ~/.vimrc'

# vim
alias v='vim -p *'
alias vp='vim -p *.*'

# tree
alias t='clear; tree'
alias r='clear; tree -L 1'

# git
alias gl='git log'
alias gs='git status'
alias ga='git add -u'
alias gc='git commit -m'
alias gt='git tag -ma'
alias gp='git push --follow-tags'

